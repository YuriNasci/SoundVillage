@model IEnumerable<SoundVillage.Application.Streaming.Dto.ArtistaItemDto>
<div>
    <a class="btn btn-primary" role="button" href="/Artista/Criar">Criar Artista</a>
</div>
<div>
    <table class="table table hover">
        <thead>
            <tr>
                <td>Nome</td>
                <td>Descrição</td>
                <td>Ações</td>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Nome</td>
                    <td>@item.Descricao</td>
                    <td></td>
                </tr>
            }
        </tbody>
    </table>
</div>
